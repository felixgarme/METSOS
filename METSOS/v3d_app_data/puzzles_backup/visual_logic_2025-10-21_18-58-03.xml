                                  <next>
                                    <block type="execScript" id="kE$r9tN-hx0EN]z7q}m{">
                                      <value name="TEXT">
                                        <shadow type="textMultiline" id="-*sN$7[|/F~d%y%7!yg[">
                                          <field name="HIGHLIGHT_MODE">javascript</field>
                                          <field name="TEXT" editor-height="1" editor-width="520">// === FPS Controls Manager para Verge3D (activar / desactivar manteniendo estado original) ===&amp;#10;// Uso: window.FPSControlsManager.enable(), .disable(), .toggle()&amp;#10;&amp;#10;(function () {&amp;#10;    if (!window || !app) {&amp;#10;        console.warn("FPSControlsManager: 'app' no está disponible.");&amp;#10;        return;&amp;#10;    }&amp;#10;&amp;#10;    // Evitar recrearlo si ya existe&amp;#10;    if (window.FPSControlsManager) return;&amp;#10;&amp;#10;    var manager = {&amp;#10;        enabled: false,&amp;#10;        _savedControls: null,&amp;#10;        _savedControlsEnabled: null,&amp;#10;        _fpsControls: null,&amp;#10;        _pointerLockHandler: null,&amp;#10;        _pointerLockElement: app.container || document.body,&amp;#10;        // Parámetros por defecto (ajusta si quieres)&amp;#10;        params: {&amp;#10;            lookSpeed: 0.15,&amp;#10;            movementSpeed: 50,&amp;#10;            noFly: false,&amp;#10;            lookVertical: true,&amp;#10;            activeLook: true,&amp;#10;            constrainVertical: true,&amp;#10;            verticalMin: 1.0,&amp;#10;            verticalMax: 2.0&amp;#10;        },&amp;#10;&amp;#10;        enable: function (opts) {&amp;#10;            if (this.enabled) return;&amp;#10;            opts = opts || {};&amp;#10;&amp;#10;            // fusionar opciones si se pasaron&amp;#10;            Object.assign(this.params, opts);&amp;#10;&amp;#10;            // Guardar controles originales&amp;#10;            this._savedControls = app.controls || null;&amp;#10;            this._savedControlsEnabled = this._savedControls ? this._savedControls.enabled : null;&amp;#10;&amp;#10;            if (this._savedControls) {&amp;#10;                try { this._savedControls.enabled = false; } catch (e) { /* ignore */ }&amp;#10;            }&amp;#10;&amp;#10;            // Verificar que FirstPersonControls exista&amp;#10;            if (!v3d || !v3d.FirstPersonControls) {&amp;#10;                console.error("FPSControlsManager: v3d.FirstPersonControls no disponible.");&amp;#10;                // Restaurar original (por si se deshabiló)&amp;#10;                if (this._savedControls) this._savedControls.enabled = this._savedControlsEnabled;&amp;#10;                return;&amp;#10;            }&amp;#10;&amp;#10;            // Crear los controles FPS&amp;#10;            var containerForControls = this._pointerLockElement || document.body;&amp;#10;            this._fpsControls = new v3d.FirstPersonControls(app.camera, containerForControls);&amp;#10;&amp;#10;            // Aplicar parámetros&amp;#10;            this._fpsControls.lookSpeed = this.params.lookSpeed;&amp;#10;            this._fpsControls.movementSpeed = this.params.movementSpeed;&amp;#10;            this._fpsControls.noFly = !!this.params.noFly;&amp;#10;            this._fpsControls.lookVertical = !!this.params.lookVertical;&amp;#10;            this._fpsControls.activeLook = !!this.params.activeLook;&amp;#10;&amp;#10;            // Vertical constraints (si aplica)&amp;#10;            this._fpsControls.constrainVertical = !!this.params.constrainVertical;&amp;#10;            if (this._fpsControls.constrainVertical) {&amp;#10;                this._fpsControls.verticalMin = this.params.verticalMin;&amp;#10;                this._fpsControls.verticalMax = this.params.verticalMax;&amp;#10;            }&amp;#10;&amp;#10;            // Ponerlo como controles activos para que el loop de Verge3D lo actualice&amp;#10;            app.controls = this._fpsControls;&amp;#10;            try { this._fpsControls.enabled = true; } catch (e) { /* ignore */ }&amp;#10;&amp;#10;            // Intentar solicitar pointer lock cuando el usuario haga click en el contenedor&amp;#10;            var self = this;&amp;#10;            function requestLockOnClick(e) {&amp;#10;                // pointer lock requiere gesture del usuario; si se llama desde un evento click funcionará&amp;#10;                try {&amp;#10;                    var el = self._pointerLockElement;&amp;#10;                    if (el &amp;&amp; el.requestPointerLock) {&amp;#10;                        el.requestPointerLock();&amp;#10;                    }&amp;#10;                } catch (err) { /* ignore */ }&amp;#10;                // quitamos el listener de click para no repetir la petición&amp;#10;                el &amp;&amp; el.removeEventListener &amp;&amp; el.removeEventListener('click', requestLockOnClick);&amp;#10;            }&amp;#10;            (this._pointerLockElement || document.body).addEventListener('click', requestLockOnClick, { once: true });&amp;#10;&amp;#10;            // Handler pointerlockchange: si se pierde el lock, puedes pausar la mirada (opcional).&amp;#10;            this._pointerLockHandler = function () {&amp;#10;                // si está bloqueado =&gt; mantener activeLook, si no =&gt; dejar activeLook en false para evitar giro al mover mouse&amp;#10;                var doc = document;&amp;#10;                var locked = doc.pointerLockElement === (self._pointerLockElement || document.body);&amp;#10;                try {&amp;#10;                    if (self._fpsControls) self._fpsControls.activeLook = locked;&amp;#10;                } catch (e) {}&amp;#10;            };&amp;#10;            document.addEventListener('pointerlockchange', this._pointerLockHandler);&amp;#10;&amp;#10;            this.enabled = true;&amp;#10;            console.log("FPSControlsManager: habilitado.");&amp;#10;        },&amp;#10;&amp;#10;        disable: function () {&amp;#10;            if (!this.enabled) return;&amp;#10;&amp;#10;            // Desactivar y eliminar controles FPS&amp;#10;            try {&amp;#10;                if (this._fpsControls) {&amp;#10;                    this._fpsControls.enabled = false;&amp;#10;                    // Si los controles tienen dispose o similar, intentar limpiarlos&amp;#10;                    if (typeof this._fpsControls.dispose === 'function') {&amp;#10;                        try { this._fpsControls.dispose(); } catch (e) {}&amp;#10;                    }&amp;#10;                }&amp;#10;            } catch (e) {}&amp;#10;&amp;#10;            // Restaurar controles originales&amp;#10;            if (this._savedControls) {&amp;#10;                try {&amp;#10;                    app.controls = this._savedControls;&amp;#10;                    if (typeof this._savedControls.enabled !== 'undefined') this._savedControls.enabled = this._savedControlsEnabled;&amp;#10;                } catch (e) {&amp;#10;                    console.warn("FPSControlsManager: error restaurando controles originales", e);&amp;#10;                    app.controls = this._savedControls; // intentar al menos reasignar&amp;#10;                }&amp;#10;            } else {&amp;#10;                // Si no había controles originales, eliminar referencia&amp;#10;                try { app.controls = null; } catch (e) {}&amp;#10;            }&amp;#10;&amp;#10;            // Quitar pointer lock (si está activado)&amp;#10;            try {&amp;#10;                if (document.exitPointerLock) document.exitPointerLock();&amp;#10;            } catch (e) {}&amp;#10;&amp;#10;            // Eliminar listener pointerlockchange&amp;#10;            if (this._pointerLockHandler) {&amp;#10;                document.removeEventListener('pointerlockchange', this._pointerLockHandler);&amp;#10;                this._pointerLockHandler = null;&amp;#10;            }&amp;#10;&amp;#10;            // Limpiar referencias&amp;#10;            this._fpsControls = null;&amp;#10;            this._savedControls = null;&amp;#10;            this._savedControlsEnabled = null;&amp;#10;            this.enabled = false;&amp;#10;            console.log("FPSControlsManager: deshabilitado y controles originales restaurados.");&amp;#10;        },&amp;#10;&amp;#10;        toggle: function (opts) {&amp;#10;            if (this.enabled) this.disable();&amp;#10;            else this.enable(opts);&amp;#10;        }&amp;#10;    };&amp;#10;&amp;#10;    // Exponer en window para acceder desde Puzzles o consola&amp;#10;    window.FPSControlsManager = manager;&amp;#10;&amp;#10;    // (Opcional) bind a tecla F para alternar&amp;#10;    function onKeyToggle(e) {&amp;#10;        // evita conflictos con inputs: solo si no hay un elemento de formulario activo&amp;#10;        var tag = document.activeElement &amp;&amp; document.activeElement.tagName;&amp;#10;        if (tag === 'INPUT' || tag === 'TEXTAREA' || document.activeElement &amp;&amp; document.activeElement.isContentEditable) return;&amp;#10;&amp;#10;        if (e.code === 'KeyF') {&amp;#10;            manager.toggle();&amp;#10;        }&amp;#10;    }&amp;#10;    window.addEventListener('keydown', onKeyToggle);&amp;#10;&amp;#10;    // Log instruccional&amp;#10;    console.log("FPSControlsManager listo. Usa window.FPSControlsManager.enable()/disable()/toggle(). Presiona 'F' para alternar.");&amp;#10;})();&amp;#10;</field>
                                        </shadow>
                                      </value>
                                    </block>
